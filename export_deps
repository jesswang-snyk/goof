      "severity": "critical",
      "socialTrendAlert": false,
      "title": "Prototype Pollution",
      "originalSeverity": "medium",
      "severityWithCritical": "critical",
      "from": [
        "goof@1.0.1",
        "tap@11.1.5",
        "nyc@11.9.0",
        "caching-transform@1.0.1",
        "mkdirp@0.5.1",
